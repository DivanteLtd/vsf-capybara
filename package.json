    "vue-carousel": "^0.6.9",
    "vue-lazy-hydration": "^1.0.0-beta.9",
    "vue-no-ssr": "^0.2.2",
    "vue-offline": "^1.0.8",
    "vue-progressbar": "^0.7.5",
    "vuelidate": "^0.6.2",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "cross-env": "^6.0.3",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^6.0.1",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1"
  }
