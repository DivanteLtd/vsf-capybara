{
  "name": "@vue-storefront/theme-capybara",
  "version": "1.11.0",
  "description": "New theme for Vue Storefront based on Storefront UI",
  "main": "index.js",
  "scripts": {
    "test:unit": "echo \"No test specified yet\" && exit 0",
    "lint": "cross-env NODE_ENV=production eslint --ext .js,.vue ./ --fix",
